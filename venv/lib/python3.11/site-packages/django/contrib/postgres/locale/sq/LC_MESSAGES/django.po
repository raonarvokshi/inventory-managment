#: venv/lib/python3.11/site-packages/django/contrib/postgres/apps.py:54
msgid "PostgreSQL extensions"
msgstr "Zgjerime PostgreSQL"

#: venv/lib/python3.11/site-packages/django/contrib/postgres/fields/array.py:21
#: venv/lib/python3.11/site-packages/django/contrib/postgres/forms/array.py:17
#: venv/lib/python3.11/site-packages/django/contrib/postgres/forms/array.py:185
#, python-format
msgid "Item %(nth)s in the array did not validate:"
msgstr "Objekti %(nth)s te vargu nuk u vleftësua:"

#: venv/lib/python3.11/site-packages/django/contrib/postgres/fields/array.py:22
msgid "Nested arrays must have the same length."
msgstr "Vargjet brenda vargjesh duhet të kenë të njëjtën gjatësi."

#: venv/lib/python3.11/site-packages/django/contrib/postgres/fields/hstore.py:15
msgid "Map of strings to strings/nulls"
msgstr "Hartë vargjesh te strings/nulls"

#: venv/lib/python3.11/site-packages/django/contrib/postgres/fields/hstore.py:17
#, python-format
msgid "The value of “%(key)s” is not a string or null."
msgstr "Vlera e “%(key)s” s’është varg ose nul."

#: venv/lib/python3.11/site-packages/django/contrib/postgres/forms/hstore.py:17
msgid "Could not load JSON data."
msgstr "S’u ngarkuan dot të dhëna JSON."

#: venv/lib/python3.11/site-packages/django/contrib/postgres/forms/hstore.py:18
msgid "Input must be a JSON dictionary."
msgstr "Vlera duhet të jetë një fjalor JSON."

#: venv/lib/python3.11/site-packages/django/contrib/postgres/forms/ranges.py:42
msgid "Enter two valid values."
msgstr "Jepni dy vlera të vlefshme."

#: venv/lib/python3.11/site-packages/django/contrib/postgres/forms/ranges.py:44
msgid "The start of the range must not exceed the end of the range."
msgstr "Fillimi i një intervali s’duhet të tejkalojë fundin e një intervali."

#: venv/lib/python3.11/site-packages/django/contrib/postgres/forms/ranges.py:99
msgid "Enter two whole numbers."
msgstr "Jepni dy vlera të plota numrash."

#: venv/lib/python3.11/site-packages/django/contrib/postgres/forms/ranges.py:105
msgid "Enter two numbers."
msgstr "Jepni dy numra."

#: venv/lib/python3.11/site-packages/django/contrib/postgres/forms/ranges.py:111
msgid "Enter two valid date/times."
msgstr "Jepni dy data/kohë të vlefshme."

#: venv/lib/python3.11/site-packages/django/contrib/postgres/forms/ranges.py:117
msgid "Enter two valid dates."
msgstr "Jepni dy data të vlefshme."

#: venv/lib/python3.11/site-packages/django/contrib/postgres/validators.py:15
#, python-format
msgid ""
"List contains %(show_value)d item, it should contain no more than "
"%(limit_value)d."
msgid_plural ""
"List contains %(show_value)d items, it should contain no more than "
"%(limit_value)d."
msgstr[0] ""
"Lista përmban %(show_value)d element, duhet të përmbajë jo më shumë se "
"%(limit_value)d."
msgstr[1] ""
"Lista përmban %(show_value)d elementë, duhet të përmbajë jo më shumë se "
"%(limit_value)d."

#: venv/lib/python3.11/site-packages/django/contrib/postgres/validators.py:25
#, python-format
msgid ""
"List contains %(show_value)d item, it should contain no fewer than "
"%(limit_value)d."
msgid_plural ""
"List contains %(show_value)d items, it should contain no fewer than "
"%(limit_value)d."
msgstr[0] ""
"Lista përmban %(show_value)d element, duhet të përmbajë jo më pak "
"%(limit_value)d."
msgstr[1] ""
"Lista përmban %(show_value)d elementë, duhet të përmbajë jo më pak "
"%(limit_value)d."

#: venv/lib/python3.11/site-packages/django/contrib/postgres/validators.py:38
#, python-format
msgid "Some keys were missing: %(keys)s"
msgstr "Mungojnë ca kyçe: %(keys)s"

#: venv/lib/python3.11/site-packages/django/contrib/postgres/validators.py:39
#, python-format
msgid "Some unknown keys were provided: %(keys)s"
msgstr "Janë dhënë kyçe të panjohur: %(keys)s"

#: venv/lib/python3.11/site-packages/django/contrib/postgres/validators.py:81
#, python-format
msgid ""
"Ensure that the upper bound of the range is not greater than %(limit_value)s."
msgstr ""
"Garantoni që kufiri i sipërm i intervalit të mos jetë më i madh se "
"%(limit_value)s."

#: venv/lib/python3.11/site-packages/django/contrib/postgres/validators.py:90
#, python-format
msgid ""
"Ensure that the lower bound of the range is not less than %(limit_value)s."
msgstr ""
"Garantoni që kufiri i poshtëm i intervalit të mos jetë më i vogël se "
"%(limit_value)s."
