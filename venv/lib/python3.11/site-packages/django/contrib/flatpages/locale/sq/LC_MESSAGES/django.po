#: venv/lib/python3.11/site-packages/django/contrib/flatpages/admin.py:13
msgid "Advanced options"
msgstr "Mundësi të mëtejshme"

#: venv/lib/python3.11/site-packages/django/contrib/flatpages/apps.py:8
msgid "Flat Pages"
msgstr "Faqe të Thjeshta"

#: venv/lib/python3.11/site-packages/django/contrib/flatpages/forms.py:11
#: venv/lib/python3.11/site-packages/django/contrib/flatpages/models.py:9
msgid "URL"
msgstr "URL"

#: venv/lib/python3.11/site-packages/django/contrib/flatpages/forms.py:15
msgid ""
"Example: “/about/contact/”. Make sure to have leading and trailing slashes."
msgstr ""
"Shembull: “/rreth/kontakt/”. Sigurohuni që vendosni pjerraket përkatëse."

#: venv/lib/python3.11/site-packages/django/contrib/flatpages/forms.py:20
msgid ""
"This value must contain only letters, numbers, dots, underscores, dashes, "
"slashes or tildes."
msgstr ""
"Kjo vlerë duhet të përmbajë vetëm shkronja, numra, pika, nënvija, vija "
"ndarëse, pjerrake ose shenjën afërsisht."

#: venv/lib/python3.11/site-packages/django/contrib/flatpages/forms.py:34
msgid "Example: “/about/contact”. Make sure to have a leading slash."
msgstr "Shembull: “/mbi/kontakt”. Sigurohuni që keni pjerrake paraprirëse."

#: venv/lib/python3.11/site-packages/django/contrib/flatpages/forms.py:47
msgid "URL is missing a leading slash."
msgstr "URL-së i mungon një pjerrake në krye."

#: venv/lib/python3.11/site-packages/django/contrib/flatpages/forms.py:52
msgid "URL is missing a trailing slash."
msgstr "URL-së i mungon një pjerrake në fund."

#: venv/lib/python3.11/site-packages/django/contrib/flatpages/forms.py:69
#, python-format
msgid "Flatpage with url %(url)s already exists for site %(site)s"
msgstr "Ka tashmë një flatpage me url %(url)s për sajtin %(site)s"

#: venv/lib/python3.11/site-packages/django/contrib/flatpages/models.py:10
msgid "title"
msgstr "titull"

#: venv/lib/python3.11/site-packages/django/contrib/flatpages/models.py:11
msgid "content"
msgstr "lëndë"

#: venv/lib/python3.11/site-packages/django/contrib/flatpages/models.py:12
msgid "enable comments"
msgstr "aktivizo komente"

#: venv/lib/python3.11/site-packages/django/contrib/flatpages/models.py:14
msgid "template name"
msgstr "emër gjedheje"

#: venv/lib/python3.11/site-packages/django/contrib/flatpages/models.py:18
msgid ""
"Example: “flatpages/contact_page.html”. If this isn’t provided, the system "
"will use “flatpages/default.html”."
msgstr ""
"Shembull: “flatpages/contact_page.html”. Nëse s’është dhënë një e tillë, "
"sistemi do të përdorë 'flatpages/default.html'."

#: venv/lib/python3.11/site-packages/django/contrib/flatpages/models.py:23
msgid "registration required"
msgstr "lypset regjistrim"

#: venv/lib/python3.11/site-packages/django/contrib/flatpages/models.py:25
msgid "If this is checked, only logged-in users will be able to view the page."
msgstr ""
"Po iu vu shenjë kësaj, faqen do të jenë në gjendje ta shohin vetëm "
"përdoruesit që kanë bërë hyrjen."

#: venv/lib/python3.11/site-packages/django/contrib/flatpages/models.py:29
msgid "sites"
msgstr "sajte"

#: venv/lib/python3.11/site-packages/django/contrib/flatpages/models.py:33
msgid "flat page"
msgstr "faqe e thjeshtë"

#: venv/lib/python3.11/site-packages/django/contrib/flatpages/models.py:34
msgid "flat pages"
msgstr "faqe të thjeshta"
